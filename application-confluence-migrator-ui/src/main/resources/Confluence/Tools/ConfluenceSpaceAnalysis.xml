  <version>1.1</version>
  <title>Confluence Space Analysis</title>
  <comment/>
  <minorEdit>false</minorEdit>
  <syntaxId>xwiki/2.1</syntaxId>
  <hidden>false</hidden>
  <content>{{include reference="Confluence.Tools.Parameters" /}}

{{velocity}}
  == $services.localization.render('confluence.migrator.spaceanalysis.title') ==

  {{info}}$services.localization.render('confluence.migrator.spaceanalysis.description'){{/info}}

  #set ($spaceAnalysisHomeDoc = $xwiki.getDocument($spaceAnalysisHomePage))
  #set ($configObj = $spaceAnalysisHomeDoc.getObject($configClass))
  #set ($displayAuthenticationForm = "$!request.config" == '1')
  #set ($spaceAnalysisURL = $xwiki.getURL($migratorHomePage, 'view', '#step-2'))

  #if ($services.job.getJobStatus(['confluence', 'spaceanalysis']).state || "$!request.nb" != '')
    == $services.localization.render('confluence.migrator.spaceanalysis.job') ==
    {{include reference="Confluence.Tools.ConfluenceSpaceAnalysisJob" /}}
  #end
  #set ($nbdocs = "100")
  #if ($request.nbdocs)
    #set ($nbdocs = $request.nbdocs)
  #end
  #set ($nb = "1")
  #if ($request.nb)
    #set ($nb = $request.nb)
  #end
  #if ("$request.confirm" != 'true')
    {{html clean=false}}
      &lt;form action="${escapetool.xml($spaceAnalysisURL)}" class="xform"&gt;
        &lt;dl&gt;
          &lt;dt&gt;
            &lt;label for=""&gt;
              $services.localization.render('confluence.migrator.spaceanalysis.nbdocs')
            &lt;/label&gt;
            &lt;span class="xHint"&gt;
              $services.localization.render('confluence.migrator.spaceanalysis.nbdocshint')
            &lt;/span&gt;
          &lt;/dt&gt;
          &lt;dd&gt;
            &lt;input type="text" name="nbdocs" value="${escapetool.xml($nbdocs)}" size="5" /&gt;
          &lt;/dd&gt;
          &lt;dt&gt;
            &lt;label for=""&gt;
              $services.localization.render('confluence.migrator.spaceanalysis.start')
            &lt;/label&gt;
            &lt;span class="xHint"&gt;
              $services.localization.render('confluence.migrator.spaceanalysis.starthint')
            &lt;/span&gt;
          &lt;/dt&gt;
          &lt;dd&gt;
            &lt;input type="text" name="start" value="0" size="5"  /&gt;
          &lt;/dd&gt;
          &lt;dt&gt;
            &lt;label for=""&gt;
              $services.localization.render('confluence.migrator.spaceanalysis.nbruns')
            &lt;/label&gt;
            &lt;span class="xHint"&gt;
              $services.localization.render('confluence.migrator.spaceanalysis.nbrunshint')
            &lt;/span&gt;
          &lt;/dt&gt;
          &lt;dd&gt;
            &lt;input type="text" name="nb" value="${escapetool.xml($nb)}" size="5" /&gt;
          &lt;/dd&gt;
          &lt;dt&gt;&lt;dt&gt;
          &lt;dd&gt;
            &lt;input name="ancestor" type="checkbox" value="1"  /&gt; $services.localization.render('confluence.migrator.spaceanalysis.hierarchy')
          &lt;/dd&gt;
          &lt;dt&gt;&lt;dt&gt;
          &lt;dd&gt;
            &lt;input name="importcheck" type="checkbox" value="1"  /&gt; $services.localization.render('confluence.migrator.spaceanalysis.importcheck')
          &lt;/dd&gt;
        &lt;/dl&gt;
        &lt;input name="confirm" type="hidden" value="true" /&gt;
        &lt;input name="submit" type="submit" value="$services.localization.render('confluence.migrator.spaceanalysis.go')" class="button" /&gt;
      &lt;/form&gt;
    {{/html}}
  #else
    #set ($qs = "nb=$!{nb}&amp;nbdocs=$!{nbdocs}")
    $response.sendRedirect($doc.getURL("view", $qs))
  #end
{{/velocity}}</content>
